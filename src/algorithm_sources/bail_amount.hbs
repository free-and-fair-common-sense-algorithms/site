<span class="k">def</span> <span class="nf">extract_classes</span><span class="p">(</span><span class="n">string</span><span class="p">):</span>
    <span class="n">matches</span> <span class="o">=</span> <span class="n">class_pattern</span><span class="o">.</span><span class="n">findall</span><span class="p">(</span><span class="n">string</span><span class="p">)</span>
    <span class="k">if</span> <span class="ow">not</span> <span class="n">matches</span><span class="p">:</span>
        <span class="n">matches</span> <span class="o">=</span> <span class="p">(</span><span class="s1">&#39;&#39;</span><span class="p">,</span> <span class="s1">&#39;&#39;</span><span class="p">)</span>
    <span class="k">else</span><span class="p">:</span>
        <span class="n">matches</span> <span class="o">=</span> <span class="n">matches</span><span class="p">[</span><span class="mi">0</span><span class="p">]</span>
    <span class="n">offense_class</span><span class="p">,</span> <span class="n">crime_class</span> <span class="o">=</span> <span class="n">matches</span>
    <span class="k">return</span> <span class="n">pd</span><span class="o">.</span><span class="n">Series</span><span class="p">((</span><span class="n">offense_class</span><span class="p">,</span> <span class="n">crime_class</span><span class="p">))</span>

<span class="n">df</span><span class="p">[[</span><span class="s1">&#39;offense_class&#39;</span><span class="p">,</span> <span class="s1">&#39;crime_class&#39;</span><span class="p">]]</span> <span class="o">=</span> <span class="n">df</span><span class="o">.</span><span class="n">apply</span><span class="p">(</span><span class="k">lambda</span> <span class="n">row</span><span class="p">:</span> <span class="n">extract_classes</span><span class="p">(</span><span class="n">row</span><span class="p">[</span><span class="s1">&#39;offense&#39;</span><span class="p">]),</span> <span class="n">axis</span><span class="o">=</span><span class="mi">1</span><span class="p">)</span>
</pre></div>

