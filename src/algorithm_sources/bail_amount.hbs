        <span class="k">return</span> <span class="s1">&#39;18â€“24&#39;</span>
    <span class="k">elif</span> <span class="mi">24</span> <span class="o">&lt;</span> <span class="n">age</span> <span class="o">&lt;=</span> <span class="mi">34</span><span class="p">:</span>
        <span class="k">return</span> <span class="s1">&#39;25â€“34&#39;</span>
    <span class="k">elif</span> <span class="mi">34</span> <span class="o">&lt;</span> <span class="n">age</span> <span class="o">&lt;=</span> <span class="mi">44</span><span class="p">:</span>
